{
  "name": "Paradox Alarm Interface (Development version)",
  "version": "dev",
  "slug": "paradox_alarm_interface_dev",
  "description": "Warning. Breaking changes. Clear MQTT before upgrading from deprecated version. Supports SP/MG/EVO, Serial and IP150 connections.",
  "uart": true,
  "url": "https://github.com/ParadoxAlarmInterface/pai/tree/dev",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "application",
  "boot": "auto",
  "usb": true,
  "map": [
    "share:rw",
    "config:ro"
  ],
  "environment": {
     "PAI_CONFIG_FILE": "/data/options.json"
  },
  "ports": {
      "10000/tcp": 10000
  },
  "ports_description": {
    "10000/tcp": "For IP connection from Babyware"
  },
  "options": {
    "LOGGING_LEVEL_CONSOLE": 20,
    "LOGGING_LEVEL_FILE": 40,
    "CONNECTION_TYPE": "Serial",
    "SERIAL_PORT": "/dev/ttyUSB0",
    "SERIAL_BAUD": 9600,
    "IP_CONNECTION_HOST": "127.0.0.1",
    "IP_CONNECTION_PORT": 10000,
    "IP_CONNECTION_PASSWORD": "paradox",
    "KEEP_ALIVE_INTERVAL": 10,
    "IO_TIMEOUT": 0.5,
    "LIMITS": {
        "zone": "auto",
        "user": "1-10",
        "door": "",
        "pgm": "1-5",
        "partition": "auto",
        "module": "",
        "repeater": "",
        "keypad": "",
        "key-switch": ""
    },
    "SYNC_TIME": true,
    "SYNC_TIME_MIN_DRIFT": 120,
    "PASSWORD": "0000",
    "MQTT_ENABLE": true,
    "MQTT_HOST": "core-mosquitto",
    "MQTT_PORT": 1883,
    "MQTT_KEEPALIVE": 60,
    "MQTT_USERNAME": "pai",
    "MQTT_PASSWORD": "my_very_secure_password",
    "MQTT_HOMEASSISTANT_AUTODISCOVERY_ENABLE": true,
    "COMMAND_ALIAS": {
        "arm": "partition all arm",
        "disarm": "partition all disarm"
    },
    "MQTT_COMMAND_ALIAS": {
        "armed_home": "arm_stay",
        "armed_night": "arm_sleep",
        "armed_away": "arm",
        "disarmed": "disarm"
    },
    "HOMEASSISTANT_NOTIFICATIONS_EVENT_FILTERS": ["live,alarm,-restore", "trouble,-clock", "live,tamper"],
    "HOMEASSISTANT_PUBLISH_PARTITION_PROPERTIES": ["target_state", "current_state"],
    "HOMEASSISTANT_PUBLISH_ZONE_PROPERTIES": ["open", "tamper"],
    "PUSHBULLET_CONTACTS": [],
    "PUSHBULLET_EVENT_FILTERS": ["live,alarm,-restore", "trouble,-clock", "live,tamper"],
    "PUSHOVER_EVENT_FILTERS": ["live,alarm,-restore", "trouble,-clock", "live,tamper"],
    "PUSHOVER_BROADCAST_KEYS": [],
    "SIGNAL_CONTACTS": [],
    "SIGNAL_EVENT_FILTERS": ["live,alarm,-restore", "trouble,-clock", "live,tamper"],
    "GSM_CONTACTS": [],
    "GSM_EVENT_FILTERS": ["live,alarm,-restore", "trouble,-clock", "live,tamper"],
    "IP_INTERFACE_ENABLE": false,
    "IP_INTERFACE_PASSWORD": "paradox",
    "DUMMY_EVENT_FILTERS": []
  },
  "schema": {
    "LOGGING_LEVEL_CONSOLE": "int",
    "LOGGING_LEVEL_FILE": "int",
    "LOGGING_FILE": "str?",
    "LOGGING_FILE_MAX_SIZE": "int?",
    "LOGGING_FILE_MAX_FILES": "int?",
    "LOGGING_DUMP_PACKETS": "bool?",
    "LOGGING_DUMP_MESSAGES": "bool?",
    "LOGGING_DUMP_STATUS": "bool?",
    "LOGGING_DUMP_EVENTS": "bool?",
    "DEVELOPMENT_DUMP_MEMORY": "bool?",
    "CONNECTION_TYPE": "list(Serial|IP)",
    "SERIAL_PORT": "str?",
    "SERIAL_BAUD": "int(1200,)?",
    "IP_CONNECTION_HOST": "str?",
    "IP_CONNECTION_PORT": "port?",
    "IP_CONNECTION_PASSWORD": "str?",
    "IP_CONNECTION_SITEID": "str?",
    "IP_CONNECTION_EMAIL": "str?",
    "IP_CONNECTION_PANEL_SERIAL": "str?",
    "IP_CONNECTION_BARE": "bool?",
    "KEEP_ALIVE_INTERVAL": "int?",
    "IO_TIMEOUT": "float?",
    "LIMITS": {
        "zone": "str?",
